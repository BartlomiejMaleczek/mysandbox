        <fullName>bvd_connector__Query__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>IF (
  BEGINS( bvd_connector__BvD_Field__c , &quot;CPYCONTACTS&quot;),
  
  bvd_connector__BvD_Field__c &amp; &quot; FILTER [Filter.Name=ContactsFilter;ContactsFilter.UCI={0};ContactsFilter.HierarchicCodeToExcludeQueryString=3|4|;ContactsFilter.SourcesToExcludeQueryString=99B|59B|69B|70B|0|;ContactsFilter.HierarchicCodeQueryString=0|1|2|] USING [Parameters.Currency=SESSION;] AS &quot; &amp; bvd_connector__BvD_Field__c,

 IF (
  NOT(ISBLANK ( bvd_connector__Data_Dimension__c )) &amp;&amp;  bvd_connector__BvD_Field_Type__c = &quot;MoneyValue&quot;,
  bvd_connector__BvD_Field__c &amp; &quot; USING [Parameters.Currency=SESSION;Parameters.DimensionSelName=&quot; &amp; bvd_connector__Data_Dimension__c &amp; &quot;;&quot; &amp; bvd_connector__Data_Dimension__c &amp; &quot;.Index=0;&quot; &amp; bvd_connector__Data_Dimension__c &amp; &quot;.Index0=0;&quot; &amp; bvd_connector__Data_Dimension__c &amp; &quot;.Index1=0;] AS &quot; &amp; bvd_connector__BvD_Field__c,
  IF (
   NOT(ISBLANK( bvd_connector__Data_Dimension__c )),
   bvd_connector__BvD_Field__c &amp; &quot; USING [Parameters.DimensionSelName=&quot; &amp; bvd_connector__Data_Dimension__c &amp; &quot;;&quot; &amp; bvd_connector__Data_Dimension__c &amp; &quot;.Index=0;&quot; &amp; bvd_connector__Data_Dimension__c &amp; &quot;.Index0=0;&quot; &amp; bvd_connector__Data_Dimension__c &amp; &quot;.Index1=0;] AS &quot; &amp; bvd_connector__BvD_Field__c,
   IF (
    bvd_connector__BvD_Field_Type__c == &quot;MoneyValue&quot;,
    bvd_connector__BvD_Field__c &amp; &quot; USING [Parameters.Currency=SESSION;] AS &quot; &amp; bvd_connector__BvD_Field__c,
    bvd_connector__BvD_Field__c &amp; &quot; AS &quot; &amp; bvd_connector__BvD_Field__c
   )
  )
 )
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
